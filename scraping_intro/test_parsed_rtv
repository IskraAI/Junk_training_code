<script type="text/javascript">
  var $j = jQuery; //No need for no-conflict mode

  // Search submit
  $(document).ready( function() {
    $("#header-search-box-form").submit(function(e){
      e.preventDefault();

      var location = window.location.href;
      var portal = 'https://www.rtvslo.si';

      if(location.indexOf('dev.rtvslo.si') !== -1){
          portal = 'https://monocms.dev.rtvslo.si';
      }

      if(location.indexOf('rtvslo.si/tv') !== -1){
          if(location.indexOf('rtvslo.si/tv-capodistria') !== -1){
            portal = portal + '/tv-capodistria';
          } else {
            portal = portal + '/tv';
          }
      }

      if(location.indexOf('rtvslo.si/radio') !== -1){
          if(location.indexOf('rtvslo.si/radio-capodistria') !== -1){
            portal = portal + '/radio-capodistria';
          } else {
            portal = portal + '/radio';
          }
      }

      var q = document.getElementById("full-header-search-input");
      if(q == null || typeof q.value == 'undefined'){
        return;
      }
      if(typeof q.value !== 'string'){
        return
      }

      q = encodeURIComponent(q.value).replace(/%20/g,"+");

      var params = location.split('?');

      var search_function = 'iskalnik';
      if(typeof window.location.href === 'string' && window.location.href.indexOf('kljucne-besede') != -1){
        search_function = 'kljucne-besede';
      }
      if(typeof window.location.href === 'string' && window.location.href.indexOf('rtvslo.si/tv') != -1){
        if(typeof window.location.href === 'string' && window.location.href.indexOf('rtvslo.si/tv-capodistria') != -1){
          search_function = 'on-demand';
        } else {
          search_function = 'na-zahtevo';
        }
      }
      if(typeof window.location.href === 'string' && window.location.href.indexOf('rtvslo.si/radio') != -1) {
        if(typeof window.location.href === 'string' && window.location.href.indexOf('rtvslo.si/radio-capodistria') != -1){
          search_function = 'on-demand';
        } else {
          search_function = 'na-zahtevo';
        }
      }

      if(params.length == 1){
        return window.location.href = portal + '/' + search_function + '?q=' + q;
      }else{
        params = params[1].split('&');
        var has_q = false;
        params.map(function(item,i){
          var atom = item.split('=');
          if(atom.length == 2){
            if(atom[0] == 'q'){
              params[i] = 'q=' + q;
              has_q = true;
            }
          }
        });
        if(has_q == false){
          params.push('q=' + q)
        }

        params = params.join('&');
        return window.location.href = portal + '/' + search_function + '?' + params;
      }
    });

    /* activation of inline search box */
    let inlineSearchBox = document.querySelector('#inline-search-box');
    if(inlineSearchBox) {
      inlineSearchBox.addEventListener('click', function() {
        document.querySelector('#main-bar').classList.add('search-box-active');
        document.querySelector('#full-header-search-input').focus();
      });
    }
  });
  // END: Search submit

  // Layout modified

  jQuery(function ($) {
    var layout = "112"
    var layoutModified = "1702495309"
    var checkInterval = 300000;

    var checkModified = function () {
      var link = "https://api.rtvslo.si/misc/lastmodified/" + layout
      if (!$j.isPlayerOpen()) {
        $.get(link).then(function(data) {
          if ('response' in data) {
            if (data.response >= layoutModified) {
              console.log("page modified, reloading", data.response, "larger than", layoutModified)
              window.location.reload()
              return
            }
            console.log("page not modified", data, " smaller than ", layoutModified)
          }
        })
      }
      setTimeout(checkModified, checkInterval)
    }
    setTimeout(checkModified, checkInterval)
  });

  // Time converter
  function timeConverter(UNIX_timestamp) {

    try{
      var a = new Date(UNIX_timestamp * 1000);
      var months = ['Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec'];
      var year = a.getFullYear();
      var month = a.getMonth() + 1;//months[a.getMonth()];
      var date = a.getDate();
      var hour = a.getHours();
      var min = a.getMinutes();
      var sec = a.getSeconds();
      min = (min < 10) ? '0' + min : min;
      var time = date + '. ' + month + '. ' + year + ' | ' + hour + '.' + min;
      console.log("server time", time, UNIX_timestamp)
      $('#layout-last-update').html(time);
    }catch(e){}
  }

  let link_server_time = "https://api.rtvslo.si/misc/time";
  fetch(link_server_time)
    .then(response => response.json()).then(data => {
      if ('response' in data) {
        timeConverter(data.response);
      }
  })
  .catch(error => {
    console.error('Error fetching data:', error);
  });


  // Login
  (function () {
      if(!cookieHelper) return;

      var bodyClassList = document.body.classList;
      var avatar = document.querySelector('#user_loggend_in_avatar');
      var avatarMobile = document.querySelector('#user_loggend_in_avatar_mobile');
      var username = document.querySelector('#user_loggend_in_name');
      var usernameMobile = document.querySelector('#user_loggend_in_name_mobile');

      if (avatar == null || avatarMobile == null || username == null || usernameMobile == null)return false;

      if (bodyClassList.contains('user-logged-in') !== -1){
        var user_id = cookieHelper.getCookie('APISESSION_USER_ID');
        if(user_id !== null) {
          var url = "url('//img.rtvslo.si/_up/avatars/" + user_id + ".png')";
          avatarMobile.style.backgroundImage = avatar.style.backgroundImage = url;
        }

        var name = cookieHelper.getCookie('APISESSION_USER_NAME');
        if(name !== null){
          username.textContent = usernameMobile.textContent = decodeURIComponent(name);
        }
      }

      avatar.addEventListener('click', function(evt){
        evt.target.parentNode.classList.toggle('tbshowdropdown');
      });
  })();
  // END: Login


  // Native share button
  (function () {
    var mobile_tmp = false;
    var w = window.innerWidth
            || document.documentElement.clientWidth
            || document.body.clientWidth;
    (w < 992) ? mobile_tmp = true : mobile_tmp = false; // 992px = lg breakpoint

    if (navigator.share && mobile_tmp) {
      var shareIconsContainer = document.querySelectorAll('.share-icons-container');
      [].forEach.call(shareIconsContainer, function (el) {
        el.classList.add('supports-native-share');
      });
    }
  })();

  function nativeShare() {
    if (navigator.share) {
      var url = document.querySelector('link[rel="canonical"]') ? document.querySelector('link[rel="canonical"]').getAttribute('href') : window.location.href;
      url = url.replace(/(^\w+:|^)\/\/[^\/]*/, '');
      var title = document.querySelector('meta[property="og:title"]').content;
      title = title.replace(/(^\w+:|^)\/\/[^\/]*/, '');
      navigator.share({
        text: title,
        url: url
      }).then(function () {
        var url_reformatted = 'url=' + url;
        return  gemius_event('_action_','', 'event=native_share', url_reformatted);
      });
    }
  }
  // END: Native share button


</script>
