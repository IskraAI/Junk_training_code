<script>
  /**
   * IOS NATIVE APP - SEND ALL LINKS TO PARENT TO PROPERRLY HANDLE THEM
   */
if (parent && document.cookie && document.cookie.indexOf("showIosNative=true") != -1) {
    parent.postMessage("pageLoadEvent","*");
    //OVERIDE WINDOW.OPEN TO SEND LINKS TO PARENT
    window.open = function (open) {
      return function (url, name, features) {
          return  parent.postMessage("link::" + url,"*");
      };
    }(window.open);

    //SEND LINKS TO PARENT
    function handlerFunction(e) {
      var linkElement = e.target.closest('a');
      //console.log("linkElement", linkElement);
      if (linkElement) {
          if (!linkElement.getAttribute('href')) {
              //console.log('A link or its child was clicked and it has no href!');
              e.preventDefault(); // Stops the link from being followed
              return
          }
      }
      if (!linkElement) {
          return;
      }
      let link = linkElement.href;

      //skip www. or monocms.dev links
      if (link.indexOf('www.rtvslo.si') != -1 || link.indexOf('monocms.dev.rtvslo.si') != -1 || link.indexOf('img.rtvcdn.si') != -1 || link.indexOf('img.rtvslo.si') != -1) {
          return;
      }
      e.preventDefault(); // Stops the link from being followed{
      parent.postMessage("link::" + link, "*");
    }
    document.addEventListener('click', handlerFunction);
}
</script>
<script src="https://img.rtvslo.si/_static/r2312017/novi/jquery/jquery-3.3.1.min.js" type="text/javascript"></script>
<style>
#admin-menu-collapse-btn {
    position: fixed;
    bottom: 70px;
    height: 40px;
    width: 40px;
    border-radius: 40px 0px 0px 40px;
    right: -1px;
    border: 1px solid var(--gray);
}

#admin-menu-collapse-btn .bottom-bar-btn-text{
    display: none;
}
</style>
<script type="text/javascript">
    // Administration
    (function ($) {
        function initAdminNav() {
            var adminDropdowns = document.querySelectorAll('#adminMenu .dropdown-toggle');
            if (!adminDropdowns) return;

            if (adminDropdowns) {
                [].forEach.call(adminDropdowns, function (el) {
                    return new BSN.Dropdown(el);
                });
            }
        }

        function editorPanel(kind) {
            var menuHTML = '';
            var sectionId = false;
            var itemId = false;

            if (kind == 'admin') {
                var sectionId = 15;
                var itemId = 5043;
            }

            if (kind == 'journalist') {
                var sectionId = 15;
                var itemId = 5267;
                if(typeof cookieHelper !== 'undefined') {
                    try {
                        cookieHelper.setCookie('isJournalist', 1, 7)
                    }catch(e){}
                }
            }

            if (kind == 'rcKoper') {
                var sectionId = 15;
                var itemId = 10084;
            }

            if (sectionId !== false || itemId !== false) {
                var getMenu = 'https://api.rtvslo.si/menu/getNavigationIndex/' + sectionId + '?client_id=82013fb3a531d5414f478747c1aca622&self_id=' + itemId;

                $.get(getMenu, function (data, success) {
                    if (success) {
                        if (data && data.response) {
                            $.each(data.response, function (id, el) {
                                if (el.submenu.length > 0) {
                                    menuHTML +=
                                        '<li class="nav-item dropup">' +
                                        '<a class="nav-link dropdown-toggle" href="' + el.link + '" onclick="' + el.onclick + '" target="' + el.target + '" id="' + el.id + '" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" rel="nofollow">' + el.title + '</a>' +
                                        '<div class="dropdown-menu" aria-labelledby="' + el.title + '">';
                                    $.each(el.submenu, function (ids, els) {
                                        menuHTML += '<a class="dropdown-item" href="' + els.link + '" onclick="' + els.onclick + '"target="' + els.target + '" rel="nofollow">' + els.title + '</a>';
                                    });
                                    menuHTML += '</div>' +
                                        '</li>';
                                } else {
                                    menuHTML +=
                                        '<li class="nav-item">' +
                                        '<a class="nav-link" href="' + el.link + '" onclick="' + el.onclick + '" target="' + el.target + '" rel="nofollow">' + el.title + '</a>' +
                                        '</li>';
                                }
                            });
                        }
                    }
                });
            }

            var cachedTime = $('meta[name=cached-time]').attr("value");
            if (typeof cachedTime === 'undefined') {
                cachedTime = 'stran ni shranjena!';
            }

            var editorPanel = $('#editor-panel');
            var panel =
                '<nav id="admin-panel" class="navbar fixed-bottom navbar-expand-lg navbar-dark bg-dark navbar-admin">' +
                '<a class="navbar-brand d-none d-lg-inline" href="#" style="line-height: 1;"><span class="icon-cog"></span></a>' +
                '<div class="collapse navbar-collapse" id="adminMenuCollapse">' +
                '<ul id="adminMenu" class="navbar-nav mr-auto"></ul>' +
                '<span id="cachedTime" style="color: rgba(255,255,255,.75)">' + cachedTime + '</span>' +
                '</div>' +
                '</nav>';
            editorPanel.html(panel);

            var mobileBottomBar = $("#mobile-bottom-bar");
            var mobileBottomBarButton =
                '<a class="mobile-bottom-bar-btn" id="admin-menu-collapse-btn" href="javascript:void(0);" data-target="#adminMenuCollapse" aria-controls="adminMenuCollapse" aria-expanded="false" aria-label="Toggle navigation">' +
                    '<span class="bottom-bar-btn-icon">' +
                        '<span class="icon-cog active-icon"></span>' +
                        '<span class="icon-cog inactive-icon"></span>' +
                    '</span>' +
                    '<span class="bottom-bar-btn-text">Admin</span>' +
                '</a>';
            mobileBottomBar.prepend(mobileBottomBarButton);

            window.addEventListener("load", function(){
              $('#adminMenu').html(menuHTML);
              initAdminNav();
              BSN.Collapse("#admin-menu-collapse-btn");
            }, false);
        }

        function editorButton(kind) {

            var articleId = $('#main-container').data('id');
            var dev = '';
            if (window.location.href.indexOf('dev.rtvslo.si') > -1) {
                dev = 'dev.';
            }

            var editLink = false;
            if (kind == 'article-edit') {
                editLink = 'https://newsadmin.' + dev + 'rtvslo.si/?c_mod=newsadmin&op=news&func=edit&id=' + articleId;
                var getArticle = 'https://api.rtvslo.si/news/getItem/' + articleId + '?prepare=full&client_id=8c5205a95060a482f0fc96b9162d9e3f';
                $.ajax({
                    url: getArticle,
                    cache: false,
                    success: function (data) {
                        var colorClass = 'bg-danger';
                        var titleDesc = 'Urejanje neaktivne novice';
                        if (data.response.active == 1) {
                            colorClass = 'bg-success';
                            titleDesc = 'Urejanje aktivne novice';
                        }
                        $('#article-edit-btn > a').addClass(colorClass);
                        $('#article-edit-btn > a').attr('title', titleDesc);
                    },
                    error: function (e) {
                        console.log('e', e)
                    }
                });
            }
            if (kind == 'article-comments') {
                editLink = 'https://newsadmin.' + dev + 'rtvslo.si/?&c_mod=newsadmin&op=comments&id=' + articleId;
            }

            if (editLink !== false) {
                var editorArticlePanel = $('#editor-article-panel');
                var editorCommentsPanel = $('#editor-comments-panel');

                var btnHTML =
                    '<div id="article-edit-btn" class="edit-btn-container container">' +
                    '<a href="' + editLink + '" class="edit-btn" style="z-index:10;" rel="nofollow">Uredi</a>' +
                    '</div>';

                if (kind == 'article-edit') {
                    editorArticlePanel.html(btnHTML);
                }

                if (kind == 'article-comments') {
                    editorCommentsPanel.html(btnHTML);
                }
            }
        }

        function quickAdministration() {
            var bodyClassList = document.body.classList;
            var userID = 1;
            var userGroups = [];

            if (bodyClassList.contains('user-logged-in') !== false) {

                // if we have php-side groups vars, just render the admin
                if (userID > 1 && userGroups.length > 0) {
                    sessionStorage.setItem('userGroups', userGroups);
                    renderQuickAdministration();
                    return;
                }

                var sessionUserGroups = sessionStorage.getItem('userGroups');
                if (sessionUserGroups === null || userGroups.join(",") !== sessionUserGroups) {
                    var getGroupsOfMemberWithSession = 'https://api.rtvslo.si/users/getGroupsOfMemberWithSession?client_id=82013fb3a531d5414f478747c1aca622';
                    $.ajaxSetup({ xhrFields: { withCredentials: true } });
                    $.get(getGroupsOfMemberWithSession, function (data, success) {
                        if (success) {
                            if (data && data.response) {
                                sessionUserGroups = data.response;
                                sessionStorage.setItem('userGroups', sessionUserGroups);
                                renderQuickAdministration();
                            }
                        }
                    });
                } else {
                    renderQuickAdministration();
                }
            }
        }

        function renderQuickAdministration() {
            var userGroups = sessionStorage.getItem('userGroups');

            if (userGroups !== undefined && userGroups.length > 0) {
                var userGroupsList = userGroups.split(",");
                var isAdmin = userGroupsList.filter(function (grp) {
                    return grp === "3";
                });
                if (isAdmin.length > 0) {
                    editorPanel('admin');
                    editorButton('article-edit');
                    editorButton('article-comments');
                } else {
                    var rcKoperGroups = ["112","81","98","82"]; //RACP, RAKP, TVCP, TVKP
                    var isRCKoperAdmin = 0;
                    $.each(rcKoperGroups, function (id, groupId) {
                        var isGroupMember = userGroupsList.filter(function (grp) {
                            return grp === groupId;
                        });
                        if (isGroupMember.length > 0) {
                            isRCKoperAdmin++;
                        }
                    });

                    if (isRCKoperAdmin > 0) {
                        editorButton('article-edit');
                        editorPanel('rcKoper');
                    } else {
                        var getJournalistGroups = 'https://api.rtvslo.si/preslikave/aktualno?client_id=82013fb3a531d5414f478747c1aca622&key=journalist_groups';
                        $.get(getJournalistGroups, function (data, success) {
                            if (success) {
                                if (data && data.response) {
                                    var journalistGroups = data.response.split(',');

                                    var isJournalist = 0;
                                    $.each(journalistGroups, function (id, groupId) {
                                        var isGroupMember = userGroupsList.filter(function (grp) {
                                            return grp === groupId;
                                        });
                                        if (isGroupMember.length > 0) {
                                            isJournalist++;
                                        }
                                    });

                                    if (isJournalist > 0) {
                                        editorButton('article-edit');
                                        editorPanel('journalist');
                                    }
                                }
                            }
                        });
                    }
                }

                var isModerator = userGroupsList.filter(function (grp) {
                    return grp === "24";
                });
                var isTurnusni = userGroupsList.filter(function (grp) {
                    return grp === "21";
                });

                if (isModerator.length > 0 || isTurnusni.length > 0) {
                    editorButton('article-comments');
                }
            }
        }

        quickAdministration();

    }) (jQuery);
    // END: Administration
</script>
