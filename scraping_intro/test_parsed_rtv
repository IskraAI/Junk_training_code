<!-- iProm -->
<script>
  _ipromNS('load', {}, function (zones) {
    // Loop thru the adverts
    for (var z in zones) {
      for (var ad in zones[z]) {
        var zoneHolder = null;

        try {
          zoneHolder = document.querySelectorAll('[data-iadserver-zone="' + z + '"]')[ad];
        } catch (e) { }

        if (zoneHolder) {
          switch (true) {
            // Dynamic adverts inside featured articles
            case zoneHolder.classList.contains('dynamic-advert-featured-articles'):
              // Set the parent that advert is visible
              zoneHolder.parentElement.parentElement.classList.add('show-advert');
              break;

            // Dynamic adverts on breaking news frontpage
            case zoneHolder.classList.contains('dynamic-advert-breaking-news'):
              zoneHolder.parentElement.parentElement.classList.add('show-advert');
              zoneHolder.parentElement.parentElement.firstElementChild.classList.add('d-none');
              break;

            // Under article - hide whole .news-block
            case zoneHolder.classList.contains('dynamic-advert-article-bottom'):
              zoneHolder.parentElement.parentElement.parentElement.parentElement.classList.remove('d-none');
              break;

            case zoneHolder.classList.contains('dynamic-advert-article-inside'):
              zoneHolder.parentElement.classList.remove('d-none');
              zoneHolder.parentElement.classList.add('mb-5');
              break;

            case zoneHolder.classList.contains('billboard-advert-item'):
              zoneHolder.parentElement.classList.remove('d-none');
              break;

            // Other dynamic adverts
            case zoneHolder.classList.contains('dynamic-advert-item'):
              zoneHolder.parentElement.parentElement.classList.add('show-advert');
              break;

            default:
              break;
          }
        }
      }
    }
  });
</script>
